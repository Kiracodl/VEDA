<group id="nc">
  <about>
    <label>Non-conservative forces</label>
    <enable> PATH.choice(fts_model) != 11 | PATH.choice(fts_model) != 17</enable>
  </about>

<!-- Preisach model cannot be directly activated by the user.  Instead a particular hysteretic model -->
<!-- is selected by the user, and then VEDA generates the appropriate coefficient tables (using a matlab -->
<!-- script).  see the hysteretic hydration force for an example -->
  <!-- <boolean id="WantPreisach"> -->
  <!--   <about>  -->
  <!--     <label>LABEL Preisach</label> 			 -->
  <!--     <enable>off</enable> -->
  <!--   </about> -->
  <!--   <default>off</default> -->
  <!-- </boolean> -->
  
  <boolean id="WantTipSqueeze">
    <about> 
      <label>LABEL Tip-Sample Squeeze Film Damping </label> 			
      <description>Beta testing. Don't use.</description>
      <enable>(PATH.choice(fts_model) != 18)  &amp;&amp; (PATH.choice(fts_model) != 20)</enable>
      <disablestyle>hide</disablestyle>
    </about>
    <default>off</default>
  </boolean>

  <number id="etaliquid">
    <about>    
      <label>LABEL Viscosity (Pa-s)</label>
      <description>Liquid viscosity</description>
      <enable>PATH.group(nc).boolean(WantTipSqueeze)</enable>
      <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>0</min>
    <max>1000</max>
    <default>1e-5</default>
  </number>
