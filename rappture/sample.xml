  <group id="VEAttard">
    <about>
      <enable>PATH.choice(fts_model) == 18  ||  PATH.choice(fts_model) == 20 </enable>
    </about>

    <image>
      #include "three_element_model.png.txt"
    </image>
    
    <choice id="modulus_type">
      <about> 
	<label>LABEL Preferred modulus input</label> 			
	<description>Choose the type of data that you have.  Shear modulus is the most common for viscoelastic DMA data.</description>
      </about>

      <option>
	<about>
	  <label>Young's modulus</label>
	</about>
	<value>1</value>
      </option>

      <option>
	<about>
	  <label>Shear modulus</label>
	</about>
	<value>2</value>
      </option>

      <default>2</default>
    </choice>

    
    <choice id="VEChoiceAttard">
      <about> 
	<label>LABEL Three element model inputs</label> 			
	<description>Only the three element model is implemented for Attard.  You can choose between entering E1, E2, and eta, or E0, Einfinity, and stress relaxation time.</description>
      </about>

      <option>
	<about>
	  <label>E1, E2, eta</label>
	</about>
	<value>5</value>
      </option>

      <option>
	<about>
	  <label>E0, Einf, tau</label>
	</about>
	<value>7</value>
      </option>

      <default>7</default>
    </choice>


    <number id="threeelm_e1">
      <about>    
	<label>LABEL 1st element modulus (GPa)</label>
	<description>See schematic of which is E1 and E2.</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 5) </enable>
	    <disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>1000</max>
      <default>1</default>
    </number>

    <number id="threeelm_e2">
      <about>    
	<label>LABEL 2nd element modulus (GPa)</label>
	<description>See schematic of which is E1 and E2.</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 5) </enable>
	    <disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>1000</max>
      <default>1</default>
    </number>
        
    <number id="etasampleHertz">
      <about>    
	<label>LABEL Sample viscosity (Pa-s)</label>
	<description>Enter a value for the sample viscosity.</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 5) </enable>
	<disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>100e9</max>
      <default>1</default>
    </number>


    <number id="threeelm_e0">
      <about>    
	<label>LABEL Modulus at t=0 (GPa)</label>
	<description>Short time modulus.</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 7) </enable>
	<disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>1000</max>
      <default>1</default>
    </number>

    <number id="threeelm_einf">
      <about>    
	<label>LABEL Modulus as t->infinity (GPa)</label>
	<description>Long time modulus.</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 7) </enable>
	    <disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>1000</max>
      <default>0.7</default>
    </number>
        
    <number id="threeelm_tau_stress">
      <about>    
	<label>LABEL Stress Relaxation Time (s)</label>
	<description>Enter a value for the stress relaxation time (note, different than the creep retardation time). Note the relaxation time must be small relative to the cantilever oscillation period (i.e. sample must be able to fully relax between each tap)</description>
	<enable>(PATH.group(nc).group(VEAttard).choice(VEChoiceAttard) == 7) </enable>
	<disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>10</max>
      <default>1e-6</default>
    </number>       
  </group>

<!--______________________________________________________________ -->
  
  <group id="VEHertz">
    <about>
      <enable>(PATH.group(nc).choice(VEchoiceHertz) != 1) &amp;&amp; ( PATH.choice(fts_model) == 2 || PATH.choice(fts_model) == 3) </enable>
    </about>
    
    <image>
      #include "viscoelastic_elements.png.txt"
    </image>

    <choice id="modulus_type">
      <about> 
	<label>LABEL Preferred modulus input</label> 			
	<description>Choose the type of data that you have.  Shear modulus is the most common for viscoelastic DMA data.</description>
      </about>

      <option>
	<about>
	  <label>Young's modulus</label>
	</about>
	<value>1</value>
      </option>

      <option>
	<about>
	  <label>Shear modulus</label>
	</about>
	<value>2</value>
      </option>

      <default>2</default>
    </choice>

    <number id="threeelm_e2">
      <about>    
	<label>LABEL 2nd element modulus (GPa)</label>
	<description>For the three element model, the 'sample modulus' above is the first element (E1 or G1) and you enter the second element here (E2 or G2). See schematic of which is E1 and E2. Note, there is currently no way to enter E_0 and E_infinity for three element model, you have to use E1 and E2.</description>
	<enable>(PATH.group(nc).choice(VEchoiceHertz) == 5)  ||  PATH.choice(fts_model) == 18  ||  PATH.choice(fts_model) == 20 </enable>
	    <disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>1000</max>
      <default>1</default>
    </number>
    
    
    <number id="etasampleHertz">
      <about>    
	<label>LABEL Sample viscosity (Pa-s)</label>
	<description>Enter a value for the sample viscosity.</description>
	<enable> (( PATH.group(nc).choice(VEchoiceHertz) != 1 ) &amp;&amp; ( PATH.group(nc).choice(VEchoiceHertz) != 6 ))  ||  PATH.choice(fts_model) == 18  ||  PATH.choice(fts_model) == 20  </enable>
	<disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>0</min>
      <max>100e9</max>
      <default>1</default>
    </number>
    
    <integer id="gen_max_N">
      <about>    
	<label>LABEL Number of elements for Generalized Maxwell </label>
	<description>Enter the number of elements in the Generalized Maxwell (prony series) model.  You may enter up to 99 elements</description>
	<enable>(PATH.group(nc).choice(VEchoiceHertz) == 6)  &amp;&amp;    PATH.choice(fts_model) != 18   &amp;&amp;   PATH.choice(fts_model) != 20 </enable>     
    <disablestyle>hide</disablestyle>
      </about>
      <units> </units>
      <min>1</min>
      <max>99</max>
      <default>3</default>
    </integer>
    
    <string id="gen_max_E">
      <about>    
	<label>LABEL Generalized Maxwell modulii (GPa)</label>
	<description>Enter a comma separated list of the modulii for the generalized maxwell model. In addition to these elements, the sample modulus above counts as one element with an infinite relaxation time. </description>
	<enable>(PATH.group(nc).choice(VEchoiceHertz) == 6)  &amp;&amp;    PATH.choice(fts_model) != 18   &amp;&amp;   PATH.choice(fts_model) != 20 </enable>     
	<disablestyle>hide</disablestyle>
      </about>
      <default>1</default>
    </string>
    
    <string id="gen_max_tau">
      <about>    
	<label>LABEL Generalized Maxwell relaxation times (s)</label>
	<description>Enter a comma separated list of the relaxation times for the generalized maxwell model.</description>
	<enable>(PATH.group(nc).choice(VEchoiceHertz) == 6)  &amp;&amp;    PATH.choice(fts_model) != 18   &amp;&amp;   PATH.choice(fts_model) != 20 </enable>     
	<disablestyle>hide</disablestyle>
      </about>
      <default>1</default>
    </string>
  </group>

  <group id="linear_schematic">
    <about>    
      <enable>PATH.choice(fts_model) == 1   &amp;&amp; PATH.group(nc).choice(VEchoiceLinear) != 1 </enable>
      <disablestyle>hide</disablestyle>
    </about>
    <image>
      #include "viscoelastic_elements.png.txt"
    </image>      
    </group>
    
    <number id="etasampleLin">
    <about>    
      <label>LABEL Damping coefficient (N-s/m)</label>
      <description>Enter a viscoelasticity damping coefficient in N-s/m.</description>
      <enable>PATH.choice(fts_model) == 1   &amp;&amp; PATH.group(nc).choice(VEchoiceLinear) != 1 </enable>
    <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>1e-12</min>
    <max>1000</max>
    <default>1e-6</default>
  </number>

  <number id="k2Lin">
    <about>    
      <label>LABEL k2 Stiffness (N/m)</label>
      <description>Enter the k2 stiffness for the three element model (the number above for sample stiffness is k1).  see manual for which is k1 and which is k2.</description>
      <enable>PATH.choice(fts_model) == 1   &amp;&amp; PATH.group(nc).choice(VEchoiceLinear) == 5</enable>
    <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>0</min>
    <max>1000</max>
    <default>0</default>
  </number>


  <boolean id="WantSurfHyst">
    <about> 
      <label>LABEL Include Surface energy hystersis? (for DMT model only)</label> 
      <enable>(PATH.choice(fts_model) == 3)  || (PATH.choice(fts_model) == 4) </enable> 
      <description>To include surface energy hysteresis, select "yes."  This model was modified from a model proposed by Garcia PRL 2006. </description>
      <disablestyle>hide</disablestyle>
    </about>
    <default>off</default>
  </boolean>

  <number id="SurfHystGamma">
    <about>    
      <label>LABEL Gamma (J/m^2)</label>
      <description>The change in surface energy between the approach and retract trajectories</description>
      <enable>PATH.group(nc).boolean(WantSurfHyst)</enable>
    <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>0</min>
    <max>10</max>
    <default>20e-3</default>
  </number>
  

  <boolean id="WantHYST">
    <about> 
      <label>LABEL Include capillary forces?</label> 
      <enable>(PATH.choice(fts_model) != 9)  &amp;&amp; (PATH.choice(fts_model) != 8)  &amp;&amp; (PATH.choice(fts_model) != 4)  &amp;&amp; (PATH.choice(fts_model) != 7)  &amp;&amp; (PATH.choice(fts_model) != 18)  &amp;&amp; (PATH.choice(fts_model) != 20)  </enable>
      <disablestyle>hide</disablestyle>
      <description>To include surface hysteretic force components, select "yes."  This adhesion hysteresis model was taken from Kober et al, Rapid Research Letters, 2008.  

	Checking or unchecking this box changes the figure at the top of the page.  Scroll up to see the updated diagram.

	Capillary forces cannot be used with JRK or DMT+DLVO models at present.  Capillary forces can not be applied to Morse or Lennard Jones potential models.

	NOTE: if you receive a DDASKR error, try increasing the number of deflection points per cycle, or decrease the approach velocity.</description>
    </about>
    <default>off</default>
  </boolean>
  


  <number id="D_0">
    <about>    
      <label>LABEL Critical gap, D_0 (nm) </label>
      <description>The breaking gap at which the hysteretic force component goes to zero. </description>
      <enable>PATH.group(nc).boolean(WantHYST)</enable>
    <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>0</min>
    <max>10</max>
    <default>0.6</default>
  </number>
  
  <number id="deltaE">
    <about>    
      <label>LABEL Energy dissipated, dE (eV) </label>
      <description>The energy dissipated in the contact process during each cycle to the hysteretic force component. </description>
      <enable>PATH.group(nc).boolean(WantHYST)</enable>
    <disablestyle>hide</disablestyle>
    </about>
    <units> </units>
    <min>0</min>
    <max>1000</max>
    <default>5</default>
  </number>
    

</group>	


